[comment encoding = UTF-8 /]
[module generator('http://www.eclipse.org/uml2/3.0.0/UML')]

[import rubyonrailsGenerator::templates::helpers /]
[import rubyonrailsGenerator::templates::entites_associations /]
[import rubyonrailsGenerator::templates::migrations /]
[import rubyonrailsGenerator::templates::controllers /]

[template public main(aModel : Model)]
[comment @main/]
[aModel.generateEntitesAndAssociations() /]
[aModel.generateMigrations() /]
[aModel.generateControllers() /]
[/template]
